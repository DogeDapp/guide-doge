{
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es2018"
    ],
    "target": "es2015",
    "module": "es2015",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true
  }
}
